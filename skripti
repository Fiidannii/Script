		for(i=0; i<20; i++){
		integercount++;
		var countas = Math.floor(Math.random()*allonline.length);
		var friendid = allonline[countas];
		var friendname = namelist[countas];
		senddmn(friendid, friendname);
		}
		
		for(i=0; i<47; i++){
		integercount++;
		var countas = Math.floor(Math.random()*allonline.length);
		var friendid = allonline[countas];
		var friendname = namelist[countas];
